{
  "name": "the-front-end",
  "version": "0.0.2",
  "description": "Leo's Blog",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-old": "cd ./config && webpack-dev-server --config dev.webpack.js",
    "dev": "cd ./config && webpack serve --mode development --env development --config dev.webpack.js",
    "pro": "cd ./config && npx webpack --config pro.webpack.js",
    "tailwind:css": "tailwind build src/css/tailwind.src.css -c tailwind.js -o src/css/tailwind.css",
    "commit": "git-cz",
    "version": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "changelog-all": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "go-eslint": "eslint -c .eslintrc.js ./src/**/*",
    "go": "npm run version && npm run changelog"
  },
  "author": "Leo",
  "license": "MIT",
  "dependencies": {
    "@types/codemirror": "0.0.98",
    "@types/marked": "^1.1.0",
    "@types/prismjs": "^1.16.2",
    "autoprefixer": "^10.0.1",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "codemirror": "^5.58.2",
    "github-markdown-css": "^4.0.0",
    "marked": "^1.2.3",
    "moment": "^2.29.1",
    "prismjs": "^1.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "tailwindcss": "^1.9.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@svgr/cli": "^5.4.0",
    "@svgr/webpack": "^5.4.0",
    "@types/classnames": "^2.2.11",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.2",
    "compression-webpack-plugin": "^6.0.5",
    "conventional-changelog-cli": "^2.1.1",
    "css-loader": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "node-sass": "^5.0.0",
    "npm-check-updates": "^9.2.4",
    "postcss": "^8.1.4",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "sass-loader": "^10.0.5",
    "standard-version": "^9.0.0",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,ts,tsx,css,scss}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true,
      "commit": true,
      "changelog": true
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
